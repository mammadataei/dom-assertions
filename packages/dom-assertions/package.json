{
  "name": "dom-assertions",
  "version": "0.1.0",
  "description": "Accessible DOM assertions for testing user interfaces.",
  "author": "Mohammad Ataei <mammadataei@gmail.com>",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && unbuild",
    "test": "vitest --ui --open=false",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "keywords": [
    "testing",
    "dom",
    "assertions",
    "accessible",
    "accessibility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mammadataei/dom-assertions/",
    "directory": "packages/dom-assertions"
  },
  "bugs": {
    "url": "https://github.com/mammadataei/dom-assertions/issues"
  }
}
